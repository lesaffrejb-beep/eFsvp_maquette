{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "efsvp/faq",
  "title": "FAQ",
  "category": "efsvp-blocks",
  "icon": "editor-help",
  "description": "Accordéon FAQ alimenté par le CPT dédié",
  "keywords": ["faq", "accordion", "questions", "efsvp"],
  "version": "1.0.0",
  "textdomain": "efsvp",
  "supports": {
    "align": ["wide", "full"],
    "anchor": true
  },
  "attributes": {
    "sectionTitle": {
      "type": "string",
      "default": "Questions fréquentes"
    },
    "sectionSubtitle": {
      "type": "string",
      "default": "Tout ce qu'il faut savoir avant de collaborer"
    },
    "postsPerPage": {
      "type": "number",
      "default": -1
    }
  },
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css"
}
